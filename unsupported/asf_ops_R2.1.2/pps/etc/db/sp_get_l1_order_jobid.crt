/*=======================
	@(#)sp_get_l1_order_jobid.crt	1.1  11/21/96
=========================*/

if exists (select * from sysobjects where name = "sp_get_l1_order_jobid")
	drop proc sp_get_l1_order_jobid
go

create proc sp_get_l1_order_jobid ( @order_id int, @item_id int ) 
as
	declare @job_id int
	select @job_id = -100

	select @job_id = job_id
			from L1_orders
			where L1_orders.item_id = @item_id
			and L1_orders.order_id = @order_id

return @job_id
go
