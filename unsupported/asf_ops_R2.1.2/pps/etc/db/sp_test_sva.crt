if exists (select * from sysobjects where name = "sp_test_sva")
	drop proc sp_test_sva
go

create proc sp_test_sva
as
	declare @job_id int
	declare @str char(10)

	select @job_id = 0

	/* find a jobid */
	select @job_id = min (job_id)
			from jobs
			where job_state = "READY"
			and job_type = "SCAN"

	/* change its job state */		
	update jobs
		set job_state = "PENDING"
		where job_id = @job_id

        select @str = convert(char(3), @job_id)
	print "Change READY to PENDING : %1! ",@str

go
