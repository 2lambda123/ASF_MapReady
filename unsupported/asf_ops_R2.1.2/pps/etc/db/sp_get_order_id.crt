=========================*/

if exists (select * from sysobjects where name = "sp_get_order_id")
	drop proc sp_get_order_id
go

create proc sp_get_order_id ( @job_id int, @job_type JobTypeType)
as


	if @job_type = "L1"
	begin
		select order_id, item_id
		from L1_orders
		where job_id = @job_id	
	end

	else if @job_type = "SCAN"
	begin
                select order_id, item_id
                from scan_orders 
                where job_id = @job_id
	end

		
go
