# This file contains the metadata for satellite capture file of the same base name.
#      '?' is likely an unknown single character value.
#      '???' is likely an unknown string of characters.
#      '-999999999' is likely an unknown integer value.
#      'NaN' is likely an unknown Real value.

meta_version: 1.20

general {                                  # Begin parameters generally used in remote sensing
    sensor: RSAT-1                             # Imaging sensor
    mode: ST3                                  # Imaging mode
    processor: ASF-PGS/PREC/VERS3.4            # Name and Version of Processor
    data_type: REAL32                          # Type of samples (e.g. REAL64)
    image_data_type: AMPLITUDE_IMAGE           # Image data type (e.g. AMPLITUDE_IMAGE)
    system: big_ieee                           # System of samples (e.g. big_ieee)
    orbit: 44141                               # Orbit Number for this datatake
    orbit_direction: A                         # Ascending 'A', or descending 'D'
    frame: -15439890                           # Frame for this image [-1 if n/a]
    band_number: 0                             # Band number; first band is 0
    line_count: 8192                           # Number of lines in image
    sample_count: 8192                         # Number of samples in image
    start_line: 0                              # First line relative to original image
    start_sample: 0                            # First sample relative to original image
    x_pixel_size: 12.5                         # Range pixel size [m]
    y_pixel_size: 12.5                         # Azimuth pixel size [m]
    center_latitude: 61.169109                 # Approximate image center latitude
    center_longitude: -146.64496               # Approximate image center longitude
    re_major: 6378144                          # Major (equator) Axis of earth [m]
    re_minor: 6356754.9                        # Minor (polar) Axis of earth [m]
    bit_error_rate: 8e-07                      # Fraction of bits which are in error
    missing_lines: -999999999                  # Number of missing lines in data take
}                                          # End general

sar {                                      # Begin parameters used specifically in SAR imaging
    image_type: P                              # [S=slant range; G=ground range; P=map projected]
    look_direction: R                          # SAR Satellite look direction [R=right; L=left]
    look_count: 4                              # Number of looks to take from SLC
    deskewed: 1                                # Image moved to zero doppler? [1=yes; 0=no]
    original_line_count: 8192                  # Number of lines in original image
    original_sample_count: 8192                # Number of samples in original image
    line_increment: 1                          # Line increment for sampling
    sample_increment: 1                        # Sample increment for sampling
    range_time_per_pixel: 7.7358484392e-08     # Time per pixel in range [s]
    azimuth_time_per_pixel: -0.0018919747457   # Time per pixel in azimuth [s]
    slant_range_first_pixel: 911919.3726       # Slant range to first pixel [m]
    slant_shift: 0                             # Error correction factor, in slant range [m]
    time_shift: 15.499057117                   # Error correction factor, in time [s]
    wavelength: 0.0565646                      # SAR carrier wavelength [m]
    prf: 1354.1619873                          # Pulse Repetition Frequency [Hz]
    earth_radius: 6361760.7422                 # Earth radius at scene center [m]
    satellite_height: 7162812.5                # Satellite height from earth's center [m]
    satellite_binary_time: ???                 # Satellite Binary Time
    satellite_clock_time: ???                  # Satellite Clock Time (UTC)
    dopRangeCen: -4137.456543                  # Range doppler centroid [Hz]
    dopRangeLin: -0.1506125                    # Range doppler per range pixel [Hz/pixel]
    dopRangeQuad: 0                            # Range doppler per range pixel sq. [Hz/(pixel^2)]
    dopAzCen: -4137.456543                     # Azimuth doppler centroid [Hz]
    dopAzLin: 0                                # Azimuth doppler per azimuth pixel [Hz/pixel]
    dopAzQuad: 0                               # Azimuth doppler per azimuth pixel sq. [Hz/(pixel^2)]
}                                          # End sar

state {                                    # Begin list of state vectors for satellite, over image
    year: 2004                                 # Year of image start
    julDay: 110                                # Julian day of the year for image start
    second: 11851.956471                       # Second of the day for image start
    vector_count: 3                            # Number of state vectors below
    vector {                                   # Begin a single state vector
        time: 0                                    # Time, relative to image start [s]
        x: -3285659.3915                           # X Coordinate, earth-fixed [m]
        y: -1569743.2626                           # Y Coordinate, earth-fixed [m]
        z: 6168591.7969                            # Z Coordinate, earth-fixed [m]
        vx: 4485.9905133                           # X Velocity, earth-fixed [m/s]
        vy: 4863.0973753                           # Y Velocity, earth-fixed [m/s]
        vz: 3619.1550293                           # Z Velocity, earth-fixed [m/s]
    }                                          # End a single state vector
    vector {                                   # Begin a single state vector
        time: 7.7495269775                         # Time, relative to image start [s]
        x: -3250765.0938                           # X Coordinate, earth-fixed [m]
        y: -1532030.7231                           # Y Coordinate, earth-fixed [m]
        z: 6196437.5                               # Z Coordinate, earth-fixed [m]
        vx: 4518.816182                            # X Velocity, earth-fixed [m/s]
        vy: 4870.9857642                           # Y Velocity, earth-fixed [m/s]
        vz: 3567.1950684                           # Z Velocity, earth-fixed [m/s]
    }                                          # End a single state vector
    vector {                                   # Begin a single state vector
        time: 15.499053955                         # Time, relative to image start [s]
        x: -3215618.0235                           # X Coordinate, earth-fixed [m]
        y: -1494258.5474                           # Y Coordinate, earth-fixed [m]
        z: 6223879.3945                            # Z Coordinate, earth-fixed [m]
        vx: 4551.3585562                           # X Velocity, earth-fixed [m/s]
        vy: 4878.5221366                           # Y Velocity, earth-fixed [m/s]
        vz: 3515.0019531                           # Z Velocity, earth-fixed [m/s]
    }                                          # End a single state vector
}                                          # End the list of state vectors

projection {                               # Map Projection parameters
    type: LAMBERT_CONFORMAL_CONIC              # Projection Type
    startX: 328882.2284                        # Projection Coordinate at top-left, X direction
    startY: 1201511.3058                       # Projection Coordinate at top-left, Y direction
    perX: 15.805085764                         # Projection Coordinate per pixel, X direction
    perY: -15.780261567                        # Projection Coordinate per pixel, Y direction
    units: meters                              # Units of projection [meters, seconds]
    hem: N                                     # Hemisphere: [N=northern hemisphere; S=southern hemisphere]
    re_major: 6378137                          # Major Axis (equator) of earth [m]
    re_minor: 6377994.0812                     # Minor Axis (polar) of earth [m]
    param {                                    # Projection specific parameters
        lamcc {                                    # Begin Lambert Conformal Conic projection
            plat1: 55                                  # First standard parallel
            plat2: 65                                  # Second standard parallel
            lat0: 50                                   # Original latitude
            lon0: -154                                 # Original longitude
        }                                          # End lamcc
    }                                          # End param
}                                          # End projection

