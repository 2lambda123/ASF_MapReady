projection {                               # Map Projection parameters
    type: ALBERS_EQUAL_AREA                    # Projection Type
    startX: -3092747.9315                      # Projection Coordinate at top-left, X direction
    startY: 5097031.7299                       # Projection Coordinate at top-left, Y direction
    perX: 12.5                                 # Projection Coordinate per pixel, X direction
    perY: 12.5                                 # Projection Coordinate per pixel, Y direction
    units: meters                              # Units of projection [meters, seconds]
    hem: N                                     # Hemisphere: [N=northern hemisphere; S=southern hemisphere]
    re_major: 6378137                          # Major Axis (equator) of earth [m]
    re_minor: 6356752.3142                     # Minor Axis (polar) of earth [m]
    param {                                    # Projection specific parameters
        albers {                                   # Begin Albers Conical Equal Area projection
