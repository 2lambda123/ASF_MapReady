# This file contains the metadata for satellite capture file of the same base name.
#      '?' is likely an unknown single character value.
#      '???' is likely an unknown string of characters.
#      '-999999999' is likely an unknown integer value.
#      'nan' is likely an unknown Real value.

meta_version: 3.60

general {                                  # Begin parameters generally used in remote sensing
    name: dem_test_nodata.tif                  # File name
    sensor: GEOTIFF                            # Imaging satellite
    sensor_name: GEOTIFF                       # Imaging sensor
    mode: ???                                  # Imaging mode
    receiving_station: ???                     # Downlinking ground station
    processor: ???                             # Name and Version of Processor
    data_type: REAL32                          # Type of samples (e.g. REAL64)
    image_data_type: GEOCODED_IMAGE            # Image data type (e.g. AMPLITUDE_IMAGE)
    radiometry: AMPLITUDE                      # Radiometry (e.g. SIGMA)
    acquisition_date: ???                      # Acquisition date of the data
    orbit: -999999999                          # Orbit Number for this datatake
    orbit_direction: ?                         # Ascending 'A', or descending 'D'
    frame: -999999999                          # Frame for this image [-1 if n/a]
    band_count: 1                              # Number of bands in image
    bands: 01                                  # Band of the sensor
    line_count: 100                            # Number of lines in image
    sample_count: 100                          # Number of samples in image
    start_line: 0                              # First line relative to original image
    start_sample: 0                            # First sample relative to original image
    x_pixel_size: 6.25                         # Range pixel size [m]
    y_pixel_size: 6.25                         # Azimuth pixel size [m]
    center_latitude: 63.6764                   # Approximate image center latitude
    center_longitude: -144.8261                # Approximate image center longitude
    re_major: 6378137.000                      # Major (equator) Axis of earth [m]
    re_minor: 6356752.314                      # Minor (polar) Axis of earth [m]
    bit_error_rate: NaN                        # Fraction of bits which are in error
    missing_lines: -999999999                  # Number of missing lines in data take
    no_data: 0.0000                            # Value indicating no data for a pixel
}                                          # End general

projection {                               # Map Projection parameters
    type: UNIVERSAL_TRANSVERSE_MERCATOR        # Projection Type
    startX: 607232.238000                      # Projection Coordinate at top-left, X direction
    startY: 7063092.778000                     # Projection Coordinate at top-left, Y direction
    perX: 6.25                                 # Projection Coordinate per pixel, X direction
    perY: -6.25                                # Projection Coordinate per pixel, Y direction
    units: meters                              # Units of projection [meters, seconds, degrees]
    hem: N                                     # Hemisphere: [N=northern hemisphere; S=southern hemisphere]
    spheroid: WGS84                            # Spheroid
    re_major: 6378137.000                      # Major Axis (equator) of earth [m]
    re_minor: 6356752.314                      # Minor Axis (polar) of earth [m]
    datum: WGS84                               # Geodetic Datum
    height: 0.000                              # Height [m]
    param {                                    # Projection specific parameters
        utm {                                      # Begin Universal Transverse Mercator Projection
            zone: 6                                    # Zone Code
            false_easting: 500000.000                  # False easting [m]
            false_northing: 0.000                      # False northing [m]
            latitude: 0.0000                           # Latitude [degrees]
            longitude: -147.0000                       # Longitude [degrees]
            scale_factor: 0.9996                       # Scaling factor
        }                                          # End utm
    }                                          # End param
}                                          # End projection

location {                                 # Block containing image corner coordinates
    lat_start_near_range: 63.6793              # Latitude at image start in near range
    lon_start_near_range: -144.8322            # Longitude at image start in near range
    lat_start_far_range: 63.6791               # Latitude at image start in far range
    lon_start_far_range: -144.8195             # Longitude at image start in far range
    lat_end_near_range: 63.6737                # Latitude at image end in near range
    lon_end_near_range: -144.8326              # Longitude at image end in near range
    lat_end_far_range: 63.6735                 # Latitude at image end in far range
    lon_end_far_range: -144.8200               # Longitude at image end in far range
}                                          # End location

