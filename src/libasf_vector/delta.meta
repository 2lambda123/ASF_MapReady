# This file contains the metadata for satellite capture file of the same base name.
#      '?' is likely an unknown single character value.
#      '???' is likely an unknown string of characters.
#      '-999999999' is likely an unknown integer value.
#      'NaN' is likely an unknown Real value.

meta_version: 1.60

general {                                  # Begin parameters generally used in remote sensing
    sensor: RSAT-1                             # Imaging sensor
    mode: ST6                                  # Imaging mode
    processor: ASF-PGS/PREC/VERS3.4            # Name and Version of Processor
    data_type: REAL32                          # Type of samples (e.g. REAL64)
    image_data_type: AMPLITUDE_IMAGE           # Image data type (e.g. AMPLITUDE_IMAGE)
    system: big_ieee                           # System of samples (e.g. big_ieee)
    orbit: 30557                               # Orbit Number for this datatake
    orbit_direction: D                         # Ascending 'A', or descending 'D'
    frame: 0                                   # Frame for this image [-1 if n/a]
    band_number: 0                             # Band number; first band is 0
    line_count: 289                            # Number of lines in image
    sample_count: 289                          # Number of samples in image
    start_line: 3088                           # First line relative to original image
    start_sample: 2472                         # First sample relative to original image
    x_pixel_size: 25                           # Range pixel size [m]
    y_pixel_size: 25                           # Azimuth pixel size [m]
    center_latitude: 63.923107                 # Approximate image center latitude
    center_longitude: -145.3084                # Approximate image center longitude
    re_major: 6378144                          # Major (equator) Axis of earth [m]
    re_minor: 6356754.9                        # Minor (polar) Axis of earth [m]
    bit_error_rate: 8e-07                      # Fraction of bits which are in error
    missing_lines: -999999999                  # Number of missing lines in data take
}                                          # End general

sar {                                      # Begin parameters used specifically in SAR imaging
    image_type: P                              # [S=slant range; G=ground range; P=map projected]
    look_direction: R                          # SAR Satellite look direction [R=right; L=left]
    look_count: 4                              # Number of looks to take from SLC
    deskewed: 1                                # Image moved to zero doppler? [1=yes; 0=no]
    original_line_count: 8192                  # Number of lines in original image
    original_sample_count: 8192                # Number of samples in original image
    line_increment: 1                          # Line increment for sampling
    sample_increment: 1                        # Sample increment for sampling
    range_time_per_pixel: 1.5471696878e-07     # Time per pixel in range [s]
    azimuth_time_per_pixel: -0.0037949915896   # Time per pixel in azimuth [s]
    slant_range_first_pixel: 1028595.8252      # Slant range to first pixel [m]
    slant_shift: 0                             # Error correction factor, in slant range [m]
    time_shift: 15.544285551                   # Error correction factor, in time [s]
    wavelength: 0.0565646                      # SAR carrier wavelength [m]
    prf: 1338.1811523                          # Pulse Repetition Frequency [Hz]
    earth_radius: 6360916.0156                 # Earth radius at scene center [m]
    satellite_height: 7162521.2402             # Satellite height from earth's center [m]
    satellite_binary_time: ???                 # Satellite Binary Time
    satellite_clock_time: ???                  # Satellite Clock Time (UTC)
    dopRangeCen: 5361.3623047                  # Range doppler centroid [Hz]
    dopRangeLin: 0.228575                      # Range doppler per range pixel [Hz/pixel]
    dopRangeQuad: 0                            # Range doppler per range pixel sq. [Hz/(pixel^2)]
    dopAzCen: 5361.3623047                     # Azimuth doppler centroid [Hz]
    dopAzLin: 0                                # Azimuth doppler per azimuth pixel [Hz/pixel]
    dopAzQuad: 0                               # Azimuth doppler per azimuth pixel sq. [Hz/(pixel^2)]
    azimuth_bandwidth: 1070.5449219            # Azimuth processing bandwidth [Hz]
    chirp_rate: -1.7622839e+12                 # Chirp rate [Hz/sec]
    pulse_duration: 4.2e-06                    # Pulse duration [s]
    range_samp_rate: 12926830.3                # Range sampling rate [Hz]
}                                          # End sar

state {                                    # Begin list of state vectors for satellite, over image
    year: 2001                                 # Year of image start
    julDay: 254                                # Julian day of the year for image start
    second: 57449.304688                       # Second of the day for image start
    vector_count: 3                            # Number of state vectors below
    vector {                                   # Begin a single state vector
        time: 0                                    # Time, relative to image start [s]
        x: -2211520.2038                           # X Coordinate, earth-fixed [m]
        y: -2430623.6765                           # Y Coordinate, earth-fixed [m]
        z: 6364736.8164                            # Z Coordinate, earth-fixed [m]
        vx: -6191.3250033                          # X Velocity, earth-fixed [m/s]
        vy: -2843.7929617                          # Y Velocity, earth-fixed [m/s]
        vz: -3230.404541                           # Z Velocity, earth-fixed [m/s]
    }                                          # End a single state vector
    vector {                                   # Begin a single state vector
        time: 7.7721405029                         # Time, relative to image start [s]
        x: -2259576.2388                           # X Coordinate, earth-fixed [m]
        y: -2452622.8508                           # Y Coordinate, earth-fixed [m]
        z: 6339421.3867                            # Z Coordinate, earth-fixed [m]
        vx: -6175.7765649                          # X Velocity, earth-fixed [m/s]
        vy: -2816.3128492                          # Y Velocity, earth-fixed [m/s]
        vz: -3283.9482422                          # Z Velocity, earth-fixed [m/s]
    }                                          # End a single state vector
    vector {                                   # Begin a single state vector
        time: 15.544281006                         # Time, relative to image start [s]
        x: -2307509.5198                           # X Coordinate, earth-fixed [m]
        y: -2474407.818                            # Y Coordinate, earth-fixed [m]
        z: 6313690.4297                            # Z Coordinate, earth-fixed [m]
        vx: -6159.7954372                          # X Velocity, earth-fixed [m/s]
        vy: -2788.6679238                          # Y Velocity, earth-fixed [m/s]
        vz: -3337.2753906                          # Z Velocity, earth-fixed [m/s]
    }                                          # End a single state vector
}                                          # End the list of state vectors

projection {                               # Map Projection parameters
    type: UNIVERSAL_TRANSVERSE_MERCATOR        # Projection Type
    startX: 594998.29995                       # Projection Coordinate at top-left, X direction
    startY: 7081523.7194                       # Projection Coordinate at top-left, Y direction
    perX: 25                                   # Projection Coordinate per pixel, X direction
    perY: -25                                  # Projection Coordinate per pixel, Y direction
    units: meters                              # Units of projection [meters, seconds, degrees]
    hem: N                                     # Hemisphere: [N=northern hemisphere; S=southern hemisphere]
    spheroid: BESSEL                           # Spheroid
    re_major: 6378137                          # Major Axis (equator) of earth [m]
    re_minor: 6356752.3142                     # Minor Axis (polar) of earth [m]
    datum: WGS84                               # Geodetic Datum
    height: 0                                  # Height [m]
    param {                                    # Projection specific parameters
        utm {                                      # Begin Universal Transverse Mercator Projection
            zone: 6                                    # Zone Code
            false_easting: 500000                      # False easting [m]
            false_northing: 0                          # False northing [m]
            latitude: 63.923107                        # Latitude [degrees]
            longitude: -145.3084                       # Longitude [degrees]
            scale_factor: 0.9996                       # Scaling factor
        }                                          # End utm
    }                                          # End param
}                                          # End projection

