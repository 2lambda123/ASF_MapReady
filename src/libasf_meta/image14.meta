# This file contains the metadata for satellite capture file of the same base name.
#      '?' is likely an unknown single character value.
#      '???' is likely an unknown string of characters.
#      '-999999999' is likely an unknown integer value.
#      'NaN' is likely an unknown Real value.

meta_version: 1.40

general {                                  # Begin parameters generally used in remote sensing
    sensor: RSAT-1                             # Imaging sensor
    mode: FN1                                  # Imaging mode
    processor: ASF-PGS/PREC/VERS3.4            # Name and Version of Processor
    data_type: REAL32                          # Type of samples (e.g. REAL64)
    image_data_type: AMPLITUDE_IMAGE           # Image data type (e.g. AMPLITUDE_IMAGE)
    system: big_ieee                           # System of samples (e.g. big_ieee)
    orbit: 25344                               # Orbit Number for this datatake
    orbit_direction: A                         # Ascending 'A', or descending 'D'
    frame: 2130640639                          # Frame for this image [-1 if n/a]
    band_number: 0                             # Band number; first band is 0
    line_count: 8192                           # Number of lines in image
    sample_count: 8192                         # Number of samples in image
    start_line: 0                              # First line relative to original image
    start_sample: 0                            # First sample relative to original image
    x_pixel_size: 6.25                         # Range pixel size [m]
    y_pixel_size: 6.25                         # Azimuth pixel size [m]
    center_latitude: -8.367137                 # Approximate image center latitude
    center_longitude: -66.559273               # Approximate image center longitude
    re_major: 6378144                          # Major (equator) Axis of earth [m]
    re_minor: 6356754.9                        # Minor (polar) Axis of earth [m]
    bit_error_rate: 0                          # Fraction of bits which are in error
    missing_lines: -999999999                  # Number of missing lines in data take
}                                          # End general

sar {                                      # Begin parameters used specifically in SAR imaging
    image_type: G                              # [S=slant range; G=ground range; P=map projected]
    look_direction: R                          # SAR Satellite look direction [R=right; L=left]
    look_count: 1                              # Number of looks to take from SLC
    deskewed: 1                                # Image moved to zero doppler? [1=yes; 0=no]
    original_line_count: 8192                  # Number of lines in original image
    original_sample_count: 8192                # Number of samples in original image
    line_increment: 1                          # Line increment for sampling
    sample_increment: 1                        # Sample increment for sampling
    range_time_per_pixel: 3.0943388251e-08     # Time per pixel in range [s]
    azimuth_time_per_pixel: -0.00094658593078  # Time per pixel in azimuth [s]
    slant_range_first_pixel: 969691.7725       # Slant range to first pixel [m]
    slant_shift: 0                             # Error correction factor, in slant range [m]
    time_shift: 7.754431945                    # Error correction factor, in time [s]
    wavelength: 0.0565646                      # SAR carrier wavelength [m]
    prf: 1293.5107422                          # Pulse Repetition Frequency [Hz]
    earth_radius: 6377694.8242                 # Earth radius at scene center [m]
    satellite_height: 7174405.4565             # Satellite height from earth's center [m]
    satellite_binary_time: ???                 # Satellite Binary Time
    satellite_clock_time: ???                  # Satellite Clock Time (UTC)
    dopRangeCen: -9375.2763672                 # Range doppler centroid [Hz]
    dopRangeLin: -0.1088813                    # Range doppler per range pixel [Hz/pixel]
    dopRangeQuad: 0                            # Range doppler per range pixel sq. [Hz/(pixel^2)]
    dopAzCen: -9375.2763672                    # Azimuth doppler centroid [Hz]
    dopAzLin: 0                                # Azimuth doppler per azimuth pixel [Hz/pixel]
    dopAzQuad: 0                               # Azimuth doppler per azimuth pixel sq. [Hz/(pixel^2)]
    azimuth_bandwidth: 1034.8085938            # Azimuth processing bandwidth [Hz]
    chirp_rate: -4.5328693e+12                 # Chirp rate [Hz/sec]
    pulse_duration: 4.2e-05                    # Pulse duration [s]
    range_samp_rate: 32317081.5                # Range sampling rate [Hz]
}                                          # End sar

