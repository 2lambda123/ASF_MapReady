meta_version: 0.9

geo {                                      # begin parameters used in geolocating the image.
    type: P                                    # Image type: [S=slant range; G=ground range; P=map projected]
    proj {                                     # Map Projection parameters
        type: L                                    # Projection Type: [U=utm; P=ps; L=Lambert; A=at/ct]
        startX: 328882.2284                        # Projection Coordinate at top-left, X direction
        startY: 1201511.3058                       # Projection Coordinate at top-left, Y direction
        perX: 15.805085764                         # Projection Coordinate per pixel, X direction
        perY: -15.780261567                        # Projection Coordinate per pixel, X direction
        hem: N                                     # Hemisphere: [N=northern hemisphere; S=southern hemisphere]
        re_major: 6378137                          # Major (equator) Axis of earth (meters)
        re_minor: 6377994.0812                     # Minor (polar) Axis of earth (meters)
        lam_plat1: 55                              # Lambert first standard parallel
        lam_plat2: 65                              # Lambert second standard parallel
        lam_lat: 50                                # Lambert original latitude
        lam_lon: -154                              # Lambert original longitude
    }                                          # end proj
    lookDir: R                                 # SAR Satellite look direction (normally R) [R=right; L=left]
    deskew: 1                                  # Image moved to zero doppler? [1=yes; 0=no]
    xPix: 12.5                                 # Pixel size in X direction [m]
    yPix: 12.5                                 # Pixel size in Y direction [m]
    rngPixTime: 7.7358484392e-08               # Time/pixel, range (xPix/(c/2.0), or 1/fs) [s]
    azPixTime: -0.0018919747457                # Time/pixel, azimuth (yPix/swathVel, or 1/prf) [s]
    slantShift: 0                              # Error correction factor, in slant range [m]
    timeShift: 15.499057117                    # Error correction factor, in time [s]
    slantFirst: 911919.3726                    # Slant range to first image pixel [m]
    wavelength: 0.0565646                      # SAR Carrier Wavelength [m]
    dopRangeCen: -4137.456543                  # Doppler centroid [Hz]
    dopRangeLin: -0.1506125                    # Doppler per range pixel [Hz/pixel]
    dopRangeQuad: 0                            # Doppler per range pixel sq. [Hz/(pixel^2)]
    dopAzCen: -4137.456543                     # Doppler centroid [Hz]
    dopAzLin: 0                                # Doppler per azimuth pixel [Hz/pixel]
    dopAzQuad: 0                               # Doppler per azimuth pixel sq. [Hz/(pixel^2)]
}                                          # end geo

ifm {                                      # begin interferometry-related parameters
    er: 6361760.7422                           # Local earth radius [m]
    ht: 7162812.5                              # Satellite height, from center of earth [m]
    nLooks: 4                                  # Number of looks to take from SLC
    orig_lines: 8192                           # Number of lines in original image
    orig_samples: 8192                         # Number of samples in original image
}                                          # end ifm

state {                                    # begin list of state vectors for satellite, over image
    year: 2004                                 # Year of image start
    day: 110                                   # Julian day of the year for image start
    second: 11851.956471                       # Second of the day for image start
    number: 3                                  # Number of state vectors below
    vector {                                   # begin a single state vector
        time: 0                                    # Time, relative to image start [s]
        x: -3285659.3915                           # X Coordinate, earth-fixed [m]
        y: -1569743.2626                           # Y Coordinate, earth-fixed [m]
        z: 6168591.7969                            # Z Coordinate, earth-fixed [m]
        vx: 4485.9905133                           # X Velocity, earth-fixed [m/s]
        vy: 4863.0973753                           # Y Velocity, earth-fixed [m/s]
        vz: 3619.1550293                           # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
    vector {                                   # begin a single state vector
        time: 7.7495269775                         # Time, relative to image start [s]
        x: -3250765.0938                           # X Coordinate, earth-fixed [m]
        y: -1532030.7231                           # Y Coordinate, earth-fixed [m]
        z: 6196437.5                               # Z Coordinate, earth-fixed [m]
        vx: 4518.816182                            # X Velocity, earth-fixed [m/s]
        vy: 4870.9857642                           # Y Velocity, earth-fixed [m/s]
        vz: 3567.1950684                           # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
    vector {                                   # begin a single state vector
        time: 15.499053955                         # Time, relative to image start [s]
        x: -3215618.0235                           # X Coordinate, earth-fixed [m]
        y: -1494258.5474                           # Y Coordinate, earth-fixed [m]
        z: 6223879.3945                            # Z Coordinate, earth-fixed [m]
        vx: 4551.3585562                           # X Velocity, earth-fixed [m/s]
        vy: 4878.5221366                           # Y Velocity, earth-fixed [m/s]
        vz: 3515.0019531                           # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
}                                          # end of list of state vectors

extra {                                    # begin extra sensor information
    sensor: RSAT-1                             # Imaging sensor
    mode: ST3                                  # Imaging mode
    processor: ASF-PGS/PREC/VERS3.4            # Name & Version of SAR Processor
    orbit: 44141                               # Orbit Number for this datatake
    bitErrorRate: 8e-07                        # Bit Error Rate
    satBinTime: ???                            # Satellite Binary Time
    satClkTime: ???                            # Satellite Clock Time (UTC)
    prf: 1354.1619873                          # Pulse Repition Frequency
}                                          # end extra

