meta_version: 0.9

geo {                                      # begin parameters used in geolocating the image.
    type: P                                    # Image type: [S=slant range; G=ground range; P=map projected]
    proj {                                     # Map Projection parameters
        type: U                                    # Projection Type: [U=utm; P=ps; L=Lambert; A=at/ct]
        startX: 379319.59121                       # Projection Coordinate at top-left, X direction
        startY: 7481636.381                        # Projection Coordinate at top-left, Y direction
        perX: 13.94497425                          # Projection Coordinate per pixel, X direction
        perY: -13.885138984                        # Projection Coordinate per pixel, X direction
        hem: N                                     # Hemisphere: [N=northern hemisphere; S=southern hemisphere]
        re_major: 6378137                          # Major (equator) Axis of earth (meters)
        re_minor: 6377994.0812                     # Minor (polar) Axis of earth (meters)
        utm_zone: 16                               # UTM Zone Code
    }                                          # end proj
    lookDir: R                                 # SAR Satellite look direction (normally R) [R=right; L=left]
    deskew: 1                                  # Image moved to zero doppler? [1=yes; 0=no]
    xPix: 12.5                                 # Pixel size in X direction [m]
    yPix: 12.5                                 # Pixel size in Y direction [m]
    rngPixTime: 7.7358484392e-08               # Time/pixel, range (xPix/(c/2.0), or 1/fs) [s]
    azPixTime: -0.0018996099196                # Time/pixel, azimuth (yPix/swathVel, or 1/prf) [s]
    slantShift: 0                              # Error correction factor, in slant range [m]
    timeShift: 15.561604461                    # Error correction factor, in time [s]
    slantFirst: 1068114.0137                   # Slant range to first image pixel [m]
    wavelength: 0.0565646                      # SAR Carrier Wavelength [m]
    dopRangeCen: -4267.6386719                 # Doppler centroid [Hz]
    dopRangeLin: -0.00185                      # Doppler per range pixel [Hz/pixel]
    dopRangeQuad: 0                            # Doppler per range pixel sq. [Hz/(pixel^2)]
    dopAzCen: -4267.6386719                    # Doppler centroid [Hz]
    dopAzLin: 0                                # Doppler per azimuth pixel [Hz/pixel]
    dopAzQuad: 0                               # Doppler per azimuth pixel sq. [Hz/(pixel^2)]
}                                          # end geo

ifm {                                      # begin interferometry-related parameters
    er: 6359790.0391                           # Local earth radius [m]
    ht: 7162211.1206                           # Satellite height, from center of earth [m]
    nLooks: 4                                  # Number of looks to take from SLC
    orig_lines: 8192                           # Number of lines in original image
    orig_samples: 8192                         # Number of samples in original image
}                                          # end ifm

state {                                    # begin list of state vectors for satellite, over image
    year: 2002                                 # Year of image start
    day: 106                                   # Julian day of the year for image start
    second: 85079.961198                       # Second of the day for image start
    number: 3                                  # Number of state vectors below
    vector {                                   # begin a single state vector
        time: 0                                    # Time, relative to image start [s]
        x: -712761.71017                           # X Coordinate, earth-fixed [m]
        y: -2868550.3968                           # Y Coordinate, earth-fixed [m]
        z: 6524384.7656                            # Z Coordinate, earth-fixed [m]
        vx: -1298.0808348                          # X Velocity, earth-fixed [m/s]
        vy: 6852.1385246                           # Y Velocity, earth-fixed [m/s]
        vz: 2864.6269531                           # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
    vector {                                   # begin a single state vector
        time: 7.7808074951                         # Time, relative to image start [s]
        x: -722803.82089                           # X Coordinate, earth-fixed [m]
        y: -2815137.6951                           # Y Coordinate, earth-fixed [m]
        z: 6546459.4727                            # Z Coordinate, earth-fixed [m]
        vx: -1284.27306                            # X Velocity, earth-fixed [m/s]
        vy: 6877.4694671                           # Y Velocity, earth-fixed [m/s]
        vz: 2809.4685059                           # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
    vector {                                   # begin a single state vector
        time: 15.56161499                          # Time, relative to image start [s]
        x: -732738.0999                            # X Coordinate, earth-fixed [m]
        y: -2761529.4078                           # Y Coordinate, earth-fixed [m]
        z: 6568104.4922                            # Z Coordinate, earth-fixed [m]
        vx: -1270.3527893                          # X Velocity, earth-fixed [m/s]
        vy: 6902.3352499                           # Y Velocity, earth-fixed [m/s]
        vz: 2754.1242676                           # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
}                                          # end of list of state vectors

extra {                                    # begin extra sensor information
    sensor: RSAT-1                             # Imaging sensor
    mode: ST7                                  # Imaging mode
    processor: ASF-PGS/PREC/VERS3.4            # Name & Version of SAR Processor
    orbit: 33663                               # Orbit Number for this datatake
    bitErrorRate: 0                            # Bit Error Rate
    satBinTime: ???                            # Satellite Binary Time
    satClkTime: ???                            # Satellite Clock Time (UTC)
    prf: 1280.8988037                          # Pulse Repition Frequency
}                                          # end extra

