meta_version: 0.9

geo {                                      # begin parameters used in geolocating the image.
    type: P                                    # Image type: [S=slant range; G=ground range; P=map projected]
    lookDir: R                                 # SAR Satellite look direction (normally R) [R=right; L=left]
    deskew: 1                                  # Image moved to zero doppler? [1=yes; 0=no]
    xPix: 12.5                                 # Pixel size in X direction [m]
    yPix: 12.5                                 # Pixel size in Y direction [m]
    rngPixTime: 7.7358484392e-08               # Time/pixel, range (xPix/(c/2.0), or 1/fs) [s]
    azPixTime: -0.0018936031718                # Time/pixel, azimuth (yPix/swathVel, or 1/prf) [s]
    slantShift: 0                              # Error correction factor, in slant range [m]
    timeShift: 15.512397183                    # Error correction factor, in time [s]
    slantFirst: 962720.3369                    # Slant range to first image pixel [m]
    wavelength: 0.0565646                      # SAR Carrier Wavelength [m]
    dopRangeCen: 6893.6459961                  # Doppler centroid [Hz]
    dopRangeLin: 0.1789875                     # Doppler per range pixel [Hz/pixel]
    dopRangeQuad: 0                            # Doppler per range pixel sq. [Hz/(pixel^2)]
    dopAzCen: 6893.6459961                     # Doppler centroid [Hz]
    dopAzLin: 0                                # Doppler per azimuth pixel [Hz/pixel]
    dopAzQuad: 0                               # Doppler per azimuth pixel sq. [Hz/(pixel^2)]
}                                          # end geo

ifm {                                      # begin interferometry-related parameters
    er: 6366098.1445                           # Local earth radius [m]
    ht: 7164610.7788                           # Satellite height, from center of earth [m]
    nLooks: 4                                  # Number of looks to take from SLC
    orig_lines: 8192                           # Number of lines in original image
    orig_samples: 8192                         # Number of samples in original image
}                                          # end ifm

state {                                    # begin list of state vectors for satellite, over image
    year: 2001                                 # Year of image start
    day: 332                                   # Julian day of the year for image start
    second: 22943.348802                       # Second of the day for image start
    number: 3                                  # Number of state vectors below
    vector {                                   # begin a single state vector
        time: 0                                    # Time, relative to image start [s]
        x: 4789867.4718                            # X Coordinate, earth-fixed [m]
        y: 341758.22997                            # Y Coordinate, earth-fixed [m]
        z: 5317750                                 # Z Coordinate, earth-fixed [m]
        vx: 5532.35867                             # X Velocity, earth-fixed [m/s]
        vy: -1620.6803644                          # Y Velocity, earth-fixed [m/s]
        vz: -4867.1220703                          # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
    vector {                                   # begin a single state vector
        time: 7.7561950684                         # Time, relative to image start [s]
        x: 4832613.8492                            # X Coordinate, earth-fixed [m]
        y: 329160.05617                            # Y Coordinate, earth-fixed [m]
        z: 5279826.6602                            # Z Coordinate, earth-fixed [m]
        vx: 5490.284248                            # X Velocity, earth-fixed [m/s]
        vy: -1629.7117295                          # Y Velocity, earth-fixed [m/s]
        vz: -4911.6567383                          # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
    vector {                                   # begin a single state vector
        time: 15.512390137                         # Time, relative to image start [s]
        x: 4875032.6589                            # X Coordinate, earth-fixed [m]
        y: 316492.6727                             # Y Coordinate, earth-fixed [m]
        z: 5241559.082                             # Z Coordinate, earth-fixed [m]
        vx: 5447.844629                            # X Velocity, earth-fixed [m/s]
        vy: -1638.5897199                          # Y Velocity, earth-fixed [m/s]
        vz: -4955.8701172                          # Z Velocity, earth-fixed [m/s]
    }                                          # end vector
}                                          # end of list of state vectors

extra {                                    # begin extra sensor information
    sensor: RSAT-1                             # Imaging sensor
    mode: ST5                                  # Imaging mode
    processor: ASF-PGS/PREC/VERS3.4            # Name & Version of SAR Processor
    orbit: 31666                               # Orbit Number for this datatake
    bitErrorRate: 8e-07                        # Bit Error Rate
    satBinTime: ???                            # Satellite Binary Time
    satClkTime: ???                            # Satellite Clock Time (UTC)
    prf: 1274.0814209                          # Pulse Repition Frequency
}                                          # end extra

